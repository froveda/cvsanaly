<h1>Listing tag_revisions</h1>

<table>
  <tr>
    <th>Tag</th>
    <th>Commit</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tag_revisions.each do |tag_revision| %>
  <tr>
    <td><%= tag_revision.tag_id %></td>
    <td><%= tag_revision.commit_id %></td>
    <td><%= link_to 'Show', tag_revision %></td>
    <td><%= link_to 'Edit', edit_tag_revision_path(tag_revision) %></td>
    <td><%= link_to 'Destroy', tag_revision, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Tag revision', new_tag_revision_path %>
